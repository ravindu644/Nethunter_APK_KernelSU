<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff303030" android:endColor="#ff303030" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff303030" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#800099cc" android:endColor="#a00099cc" android:angle="270.0" android:centerY="0.75" android:centerColor="#800099cc" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff0099cc" android:endColor="#ff0099cc" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff0099cc" />
            </shape>
        </clip>
    </item>
</layer-list>
